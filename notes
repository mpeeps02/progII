-public
-private(vor Methoden, die im eigener Klasse benutzt werden sollen)

public class Mitarbeiter{
    public String name;
    public int alter;
    private double jahresgehalt;
    private String passwort;
    
    public Mitarbeiter(String name,int alter,double jahresgehal,String passwort){
      this.namen=namen;
      this.alter=alter;
      this.jahresgehalt=jahresgehalt;
      this.passwort=passwort;
      
     public void arbeiten(){
     System.out.println("Arbeiten"):}
    
     private String geheimnis(){ alternativ mit getter/setter public double jahresgehalt(){return jahresgehalt;}
     return "geheimnis";
     }   
     public void setJahresgehalt(double neuesGehalt){
      jahresgehalt=neuesGehalt;
     }
     
 }
}

public class Main{
  public static void main(String [] args){
    Mitarbeiter peter = new Mitarbeiter (name: "peter", alter:24, jahresgehalt: 100000, passwort: 123456);
  System.out.println(peter.passwort);
  peter.setJahresgehalt()150000;
  System.out.println(peter.getJahresgehalt);
  }
}
